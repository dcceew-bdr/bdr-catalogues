PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX prof: <http://www.w3.org/ns/dx/prof/>
PREFIX schema: <https://schema.org/>

[]    a <https://prez.dev/Manifest> ;
    prof:hasResource
        [
            prof:hasArtifact "background/*.ttl" ;
            prof:hasRole <https://prez.dev/ManifestResourceRoles/IncompleteCatalogueAndResourceLabels> ;
        ] ,
        [
            dcterms:conformsTo "resources/validators/bdr/items/cats.ttl" ;
            prof:hasArtifact "resources/catalogue.ttl" ;
            prof:hasRole <https://prez.dev/ManifestResourceRoles/CatalogueData> ;
        ] ,
        [
            dcterms:conformsTo "resources/validators/bdr/items/cats.ttl" ;
            prof:hasArtifact
                "resources/datasets/catalogue.ttl" ,
                "resources/datasets/obs/catalogue.ttl" ,
                "resources/datasets/ref/catalogue.ttl" ,
                "resources/models/catalogue.ttl" ,
                "resources/models/abis/catalogue.ttl" ,
                "resources/models/bdr/catalogue.ttl" ,
                "resources/validators/catalogue.ttl" ,
                "resources/validators/abis/catalogue.ttl" ,
                "resources/validators/bdr/catalogue.ttl" ,
                "resources/vocabularies/catalogue.ttl" ;
            prof:hasRole <https://prez.dev/ManifestResourceRoles/ResourceData> ;
        ] ,
        [
            dcterms:conformsTo "resources/validators/abis/items/ontpub.ttl" ;
            prof:hasArtifact
                "resources/models/abis/items/rm.ttl" ,
                "resources/models/abis/items/nsl.ttl" ,
                # "resources/models/abis/items/tern.ttl" ,
                "resources/models/bdr/items/*.ttl" ;
            prof:hasRole <https://prez.dev/ManifestResourceRoles/ResourceData> ;
        ] ,
        [
            dcterms:conformsTo "resources/validators/abis/items/shacl.ttl" ;
            prof:hasArtifact
                # "resources/validators/abis/items/*.ttl" ;
                "resources/validators/bdr/items/*.ttl" ;
            prof:hasRole <https://prez.dev/ManifestResourceRoles/ResourceData> ;
        ] ;
.